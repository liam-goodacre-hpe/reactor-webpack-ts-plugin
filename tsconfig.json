{
  "compilerOptions": {
    "lib": [
      "es2015",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "noImplicitAny": false,
    "outDir": "spec-out",
    "strictNullChecks": true,
    "target": "es5"
  },
  "atom": {
    "rewriteTsconfig": false
  },
  "buildOnSave": false,
  "compileOnSave": false,
  "include": [
    "src/**/*",
    "custom-typings/*",
    "typings/index.d.ts"
  ]
}
